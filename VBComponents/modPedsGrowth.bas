Private Function CalcNormDist(Value As Single, L As Variant, M As Variant, S As Variant) As Single
    ' Declare variables
    Dim Z As Variant
    ' Validate variables
    If L < 0 And Value = 0 Then
        CalcNormDist = CVErr(xlErrNum)
        Exit Function
    End If
    ' Calculate Z score
    Z = (((Value / M) ^ L) - 1) / (L * S)
    ' Return normal distribution
    CalcNormDist = WorksheetFunction.NormSDist(Z) * 100
End Function

